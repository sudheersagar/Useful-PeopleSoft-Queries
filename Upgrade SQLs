WHERE ITEM.PROJECTNAME = ('ZAKTST')
AND ITEM.OBJECTTYPE = 46
AND ITEM.OBJECTVALUE1 = DEFN.OBJECTVALUE1 AND ITEM.OBJECTVALUE2 = DEFN.OBJECTVALUE2 AND ITEM.OBJECTVALUE3 = DEFN.OBJECTVALUE3
