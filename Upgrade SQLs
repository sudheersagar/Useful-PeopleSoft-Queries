-- Objects in Project not Listed
SELECT * FROM PSPROJECTITEM WHERE PROJECTNAME = 'ZAKTST'
AND OBJECTTYPE NOT IN (33, 34, 57, 32, 7, 2, 6, 25, 36, 37, 5, 43, 58, 42, 46, 48, 47, 44, 8, 40, 55, 0, 30, 4, 20, 10, 50, 51, 53);
