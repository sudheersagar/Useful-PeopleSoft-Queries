FROM PSPROJECTITEM ITEM, PSSQLDEFN DEFN
WHERE ITEM.PROJECTNAME = ('ZAKTST')
AND ITEM.OBJECTTYPE = 30 and ITEM.OBJECTVALUE2 = 1
AND ITEM.OBJECTVALUE1 = DEFN.SQLID
AND DEFN.LASTUPDDTTM > TO_DATE('2000-01-01', 'YYYY-MM-DD')
